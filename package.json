{
  "name": "bridge-react",
  "version": "0.1.6",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "http://nexus.bridge.ufsc.br/content/repositories/npm-private/"
  },
  "scripts": {
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
    "test": "jest",
    "test:watch": "jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/enzyme": "2.8.4",
    "@types/jest": "18.1.1",
    "@types/react": "15.6.0",
    "@types/react-overlays": "0.6.8",
    "@types/react-redux": "4.4.47",
    "@types/react-router": "4.0.14",
    "@types/react-select": "1.0.48",
    "@types/react-test-renderer": "15.5.3",
    "@types/redux-form": "6.6.7",
    "@types/redux-ui": "0.0.10",
    "awesome-typescript-loader": "3.2.3",
    "axios": "0.15.3",
    "babel-jest": "20.0.3",
    "bulma": "0.4.2",
    "classnames": "2.2.5",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "immutable": "3.8.1",
    "inputmask-core": "2.2.0",
    "lodash": "4.17.4",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "moment": "2.17.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-maskedinput": "4.0.0",
    "react-overlays": "0.8.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.2",
    "react-select": "1.0.0-rc.5",
    "react-test-renderer": "15.6.1",
    "redux-form": "7.0.3",
    "redux-ui": "0.1.1",
    "shx": "0.2.2",
    "ts-jest": "20.0.5",
    "tslint": "5.2.0",
    "typescript": "2.4.2",
    "webpack": "3.5.5"
  },
  "peerDependencies": {
    "axios": "0.15.x",
    "bulma": "0.4.x",
    "classnames": "2.x",
    "immutable": "3.x",
    "inputmask-core": "2.x",
    "lodash": "4.x",
    "moment": "2.x",
    "react": "15.x",
    "react-dom": "15.x",
    "react-maskedinput": "4.x",
    "react-overlays": "0.8.x",
    "react-redux": "5.x",
    "react-router-dom": "4.x",
    "react-select": "1.x",
    "redux-form": "7.x",
    "redux-ui": "0.x"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/__tests__/test.defaults.ts"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
