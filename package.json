{
  "name": "bridge-react",
  "version": "1.0.0-alpha.127",
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "homepage": "https://bold.bridge.ufsc.br",
  "files": [
    "lib",
    "icons",
    "tslint.json"
  ],
  "repository": {
    "url": "https://github.com/laboratoriobridge/bridge-react"
  },
  "publishConfig": {
    "registry": "https://repositorio.bridge.ufsc.br/repository/npm-private/"
  },
  "scripts": {
    "start": "npm run clean && webpack --progress --watch",
    "clean": "rm -rf lib",
    "build": "npm run clean && tsc && webpack --mode=production",
    "test": "npm run lint && TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "icons": "npm run icons:clean && npm run icons:svgr && npm run icons:index",
    "icons:clean": "rm -rf ./src/components/elements/Icon/generated",
    "icons:svgr": "svgr --ext tsx --icon --template ./tools/IconTemplate.js --svgo-config svgo.config.json -d ./src/components/elements/Icon/generated ./icons",
    "icons:index": "babel-node ./tools/generateIconsIndex.js",
    "sonar": "sonar-scanner",
    "site:dev": "next site/",
    "site:build": "next build site/",
    "site:export": "next build site/ && next export site/ && touch site/out/.nojekyll && cp site/CNAME site/out/CNAME && build-storybook -o site/out/storybook",
    "site:deploy": "npm run site:export && gh-pages -d site/out -t"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm test"
    }
  },
  "peerDependencies": {
    "emotion": "^10.0.9",
    "final-form": "^4.12.0",
