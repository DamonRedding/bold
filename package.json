{
  "name": "bridge-react",
  "version": "1.0.0-alpha.46",
  "main": "lib/bundle.js",
  "types": "lib/index.d.ts",
  "repository": {
    "url": "https://github.com/laboratoriobridge/bridge-react"
  },
  "publishConfig": {
    "registry": "https://repositorio.bridge.ufsc.br/repository/npm-private/"
  },
  "scripts": {
    "precommit": "npm run generate-icons && git add ./src/components/elements/Icon/generated && npm test",
    "start": "npm run clean && webpack --progress --watch",
    "clean": "shx rm -rf lib",
    "build": "npm run clean && webpack --mode=production",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-icons": "npm run clean-icons && npm run generate-svg-icons && npm run generate-icons-index",
    "clean-icons": "rm -r ./src/components/elements/Icon/generated",
    "generate-svg-icons": "svgr --ext tsx --icon --template ./icons/IconTemplate.js -d ./src/components/elements/Icon/generated ./icons/svgs",
    "generate-icons-index": "babel-node ./tools/generateIconsIndex.js",
    "sonar": "sonar-scanner"
  },
  "peerDependencies": {
    "axios": "^0.18.0",
    "emotion": "^9.2.3",
    "final-form": "^4.6.1",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-final-form": "^3.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "tslint-react": "^3.6.0"
  },
  "dependencies": {
    "emotion-theming": "9.2.5",
    "final-form-focus": "1.0.0",
    "normalize.css": "8.0.0",
    "react-datepicker": "1.5.0",
    "react-dropzone": "4.2.13",
    "react-popper": "1.0.0",
    "react-select": "1.2.1",
    "react-text-mask": "5.4.2",
    "text-mask-addons": "3.7.2",
    "text-mask-core": "5.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.11",
    "@storybook/addon-info": "4.0.0-alpha.11",
    "@storybook/addon-knobs": "4.0.0-alpha.11",
    "@storybook/addon-links": "4.0.0-alpha.11",
    "@storybook/addon-options": "4.0.0-alpha.11",
    "@storybook/addons": "4.0.0-alpha.11",
    "@storybook/react": "4.0.0-alpha.11",
    "@svgr/cli": "2.0.0",
    "@types/classnames": "2.2.4",
    "@types/enzyme": "3.1.11",
    "@types/jest": "23.1.4",
    "@types/lodash": "4.14.110",
    "@types/prop-types": "15.5.3",
    "@types/react": "16.4.6",
    "@types/react-addons-create-fragment": "0.14.18",
    "@types/react-datepicker": "1.1.5",
    "@types/react-dropzone": "4.2.0",
    "@types/react-redux": "6.0.3",
    "@types/react-router": "4.0.28",
    "@types/react-router-dom": "4.2.7",
    "@types/react-select": "1.2.8",
    "@types/redux-mock-store": "1.0.0",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-info": "3.2.3",
    "@types/storybook__addon-knobs": "3.3.1",
    "@types/storybook__addon-links": "3.3.1",
    "@types/storybook__addon-options": "3.2.2",
    "@types/storybook__react": "3.0.7",
    "awesome-typescript-loader": "5.2.0",
    "axios": "0.18.0",
    "axios-mock-adapter": "1.15.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.2.0",
    "babel-runtime": "6.26.0",
    "copyfiles": "2.0.0",
    "emotion": "9.2.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "npm:enzyme-react-adapter-future",
    "enzyme-to-json": "3.3.4",
    "file-loader": "1.1.11",
    "final-form": "4.8.3",
    "husky": "0.14.3",
    "intl": "1.2.5",
    "jest": "23.2.0",
    "jest-cli": "23.2.0",
    "jest-emotion": "9.2.4",
    "lodash": "4.17.10",
    "marksy": "6.0.3",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-addons-create-fragment": "15.6.2",
    "react-docgen-typescript": "1.6.1",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-dom": "16.4.1",
    "react-final-form": "3.6.5",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "shx": "0.3.1",
    "sonar-scanner": "3.1.0",
    "ts-jest": "23.0.0",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setupTests.ts",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(svg|woff2|ttf)$": "<rootDir>/src/test/__mocks__/fileMock.ts",
      "\\.(css)$": "<rootDir>/src/test/__mocks__/styleMock.ts"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
