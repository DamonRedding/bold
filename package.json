{
  "name": "bridge-react",
  "version": "1.0.0-alpha.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "http://nexus.bridge.ufsc.br/content/repositories/npm-private/"
  },
  "scripts": {
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
    "test": "jest",
    "test:watch": "jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.16",
    "@storybook/addon-info": "3.2.16",
    "@storybook/addon-knobs": "3.2.16",
    "@storybook/addon-links": "3.2.16",
    "@storybook/addon-options": "3.2.16",
    "@storybook/react": "3.2.16",
    "@types/classnames": "2.2.3",
    "@types/enzyme": "3.1.5",
    "@types/jest": "18.1.1",
    "@types/react": "16.0.25",
    "@types/react-overlays": "0.6.9",
    "@types/react-redux": "5.0.13",
    "@types/react-router": "4.0.18",
    "@types/react-select": "1.0.48",
    "@types/react-test-renderer": "16.0.0",
    "@types/redux-form": "7.0.9",
    "@types/redux-ui": "0.0.13",
    "@types/storybook__react": "3.0.5",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-info": "3.2.1",
    "@types/storybook__addon-knobs": "3.2.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__addon-options": "3.2.0",
    "awesome-typescript-loader": "3.2.3",
    "axios": "0.17.1",
    "babel-jest": "21.2.0",
    "bulma": "0.4.2",
    "classnames": "2.2.5",
    "enzyme": "3.2.0",
    "enzyme-to-json": "3.2.2",
    "immutable": "3.8.1",
    "inputmask-core": "2.2.0",
    "lodash": "4.17.4",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "moment": "2.17.1",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-maskedinput": "4.0.0",
    "react-overlays": "0.8.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-select": "1.0.1",
    "react-test-renderer": "16.1.1",
    "redux-form": "7.1.2",
    "redux-ui": "0.1.1",
    "shx": "0.2.2",
    "ts-jest": "20.0.5",
    "tslint": "5.2.0",
    "typescript": "2.6.1",
    "webpack": "3.5.5"
  },
  "peerDependencies": {
    "axios": "0.17.x",
    "bulma": "0.4.x",
    "classnames": "2.x",
    "immutable": "3.x",
    "inputmask-core": "2.x",
    "lodash": "4.x",
    "moment": "2.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-maskedinput": "4.x",
    "react-overlays": "0.8.x",
    "react-redux": "5.x",
    "react-router-dom": "4.x",
    "react-select": "1.x",
    "redux-form": "7.x",
    "redux-ui": "0.x"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/__tests__/test.defaults.ts"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
