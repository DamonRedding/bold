{
  "name": "bridge-react",
  "version": "1.0.0-alpha.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "http://nexus.bridge.ufsc.br/content/repositories/npm-private/"
  },
  "scripts": {
    "clean": "shx rm -rf _bundles lib",
    "copyfiles": "copyfiles -u 1 ./src/styles/theme/**/fonts/** lib/",
    "build": "npm run clean && tsc && npm run copyfiles && webpack --progress",
    "test": "jest",
    "test:watch": "jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-prop-types": "babel-node ./tools/generatePropTypes.js"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.17",
    "@storybook/addon-knobs": "3.2.17",
    "@storybook/addon-links": "3.2.17",
    "@storybook/addon-options": "3.2.17",
    "@storybook/react": "3.2.17",
    "@types/aphrodite": "0.5.6",
    "@types/classnames": "2.2.3",
    "@types/enzyme": "3.1.5",
    "@types/jest": "22.0.0",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.33",
    "@types/react-addons-create-fragment": "0.14.17",
    "@types/react-overlays": "0.6.9",
    "@types/react-redux": "5.0.13",
    "@types/react-router": "4.0.18",
    "@types/react-select": "1.0.48",
    "@types/redux-form": "7.0.9",
    "@types/redux-ui": "0.0.13",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-knobs": "3.2.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__addon-options": "3.2.0",
    "@types/storybook__react": "3.0.5",
    "aphrodite": "1.2.5",
    "awesome-typescript-loader": "3.2.3",
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-jest": "21.2.0",
    "bulma": "0.4.2",
    "classnames": "2.2.5",
    "copyfiles": "1.2.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "3.3.0",
    "immutable": "3.8.1",
    "inputmask-core": "2.2.0",
    "jest": "22.0.4",
    "jest-cli": "22.0.4",
    "marksy": "5.0.0",
    "moment": "2.17.1",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-addons-create-fragment": "15.6.2",
    "react-docgen-typescript": "1.1.0",
    "react-dom": "16.2.0",
    "react-maskedinput": "4.0.0",
    "react-overlays": "0.8.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-select": "1.0.1",
    "redux-form": "7.1.2",
    "redux-ui": "0.1.1",
    "shx": "0.2.2",
    "ts-jest": "22.0.0",
    "tslint": "5.8.0",
    "tslint-react": "3.3.3",
    "typescript": "2.6.2",
    "url-loader": "0.5.7",
    "webpack": "3.5.5"
  },
  "peerDependencies": {
    "aphrodite": "1.x",
    "axios": "0.17.x",
    "bulma": "0.4.x",
    "classnames": "2.x",
    "immutable": "3.x",
    "inputmask-core": "2.x",
    "moment": "2.x",
    "react": "16.2.x",
    "react-dom": "16.2.x",
    "react-maskedinput": "4.x",
    "react-overlays": "0.8.x",
    "react-redux": "5.x",
    "react-router-dom": "4.x",
    "react-select": "1.x",
    "redux-form": "7.x",
    "redux-ui": "0.x"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setupTests.ts",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/__tests__/test.defaults.ts"
    ],
    "testRegex": ".*\\.test\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(svg|woff2|ttf)$": "<rootDir>/src/test/__mocks__/fileMock.ts"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
