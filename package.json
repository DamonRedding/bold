{
  "name": "bridge-react",
  "version": "1.0.0-alpha.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "url": "https://github.com/laboratoriobridge/bridge-react"
  },
  "publishConfig": {
    "registry": "http://nexus.bridge.ufsc.br/content/repositories/npm-private/"
  },
  "scripts": {
    "start": "npm run build && tsc --watch",
    "clean": "shx rm -rf lib",
    "copyfiles": "copyfiles -u 1 ./src/styles/theme/**/fonts/** lib/",
    "build": "npm run clean && tsc && npm run copyfiles",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run generate-prop-types && build-storybook",
    "generate-prop-types": "babel-node ./tools/generatePropTypes.js"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.17",
    "@storybook/addon-knobs": "3.2.17",
    "@storybook/addon-links": "3.2.17",
    "@storybook/addon-options": "3.2.17",
    "@storybook/react": "3.2.17",
    "@types/classnames": "2.2.3",
    "@types/enzyme": "3.1.5",
    "@types/jest": "22.0.0",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.33",
    "@types/react-addons-create-fragment": "0.14.17",
    "@types/react-overlays": "0.6.9",
    "@types/react-redux": "5.0.13",
    "@types/react-router": "4.0.18",
    "@types/react-select": "1.0.48",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-knobs": "3.2.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__addon-options": "3.2.0",
    "@types/storybook__react": "3.0.5",
    "awesome-typescript-loader": "3.2.3",
    "axios": "0.17.1",
    "axios-mock-adapter": "1.10.0",
    "babel-cli": "6.26.0",
    "babel-jest": "21.2.0",
    "copyfiles": "1.2.0",
    "emotion": "9.0.0-1",
    "emotion-theming": "9.0.0-1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "3.3.0",
    "final-form": "4.0.3",
    "intl": "1.2.5",
    "jest": "22.0.4",
    "jest-cli": "22.0.4",
    "jest-emotion": "9.0.0-1",
    "marksy": "6.0.3",
    "moment": "2.17.1",
    "normalize.css": "7.0.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-addons-create-fragment": "15.6.2",
    "react-docgen-typescript": "1.1.0",
    "react-dom": "16.2.0",
    "react-final-form": "3.0.5",
    "react-overlays": "0.8.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux-mock-store": "1.4.0",
    "redux-thunk": "2.2.0",
    "shx": "0.2.2",
    "ts-jest": "22.0.0",
    "tslint": "5.8.0",
    "tslint-react": "3.3.3",
    "typescript": "2.6.2",
    "url-loader": "0.5.7",
    "webpack": "3.5.5"
  },
  "peerDependencies": {
    "axios": "0.17.x",
    "emotion": "9.x",
    "emotion-theming": "9.x",
    "final-form": "4.x",
    "normalize.css": "x",
    "react": "16.2.x",
    "react-dom": "16.2.x",
    "react-final-form": "3.x",
    "react-overlays": "0.8.x",
    "react-redux": "5.x"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setupTests.ts",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(svg|woff2|ttf|css)$": "<rootDir>/src/test/__mocks__/fileMock.ts"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
